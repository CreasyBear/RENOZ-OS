    // Log audit event for bulk update
    await logAuditEvent({
      organizationId: ctx.organizationId,
      userId: ctx.user.id,
      action: AUDIT_ACTIONS.USER_UPDATE,
      entityType: AUDIT_ENTITY_TYPES.USER,
      metadata: {
        bulk: true,
        userIds: validUserIds,
        updates,
        count: validUserIds.length,
      },
    });
